{
    "manifest_version": 3,
    "name": "Monitoramento de conectores do Floui",
    "version": "1.0",
    "permissions": [
      "tabs",
      "activeTab", 
      "sidePanel",
      "scripting"      
    ],
    "background": {
      "service_worker": "src/background.js"
    },
    "icons": {
      "16": "src/images/icon16.png",
      "48": "src/images/icon48.png",
      "128": "src/images/icon128.png"
    }, 
    "side_panel": {
      "default_path": "sidepanel.html"
    },    
    "action": {
      "default_icon": {
        "128": "src/images/icon128.png"
      },
      "default_title": "Floui Extension",
      "default_badge": {
        "color": "#f00",
        "text": "1"
      }
    },
    "host_permissions": [
      "https://app.*.ws.floui.io",
      "https://app.floui.io/*"
      
    ]
  }
  